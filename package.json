    "bower": "^1.8.4",
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.10",
    "grunt": "^1.0.3",
    "grunt-karma": "^3.0.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-expect": "^3.8.3",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
